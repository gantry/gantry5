    <license>http://www.gnu.org/licenses/gpl-2.0.html GNU/GPL</license>
    <description>LIB_GANTRY5_DESCRIPTION</description>

    <files folder="library">
        <filename>bootstrap.php</filename>
        <filename>Loader.php</filename>
        <filename>RealLoader.php</filename>
        <filename>composer.json</filename>
        <filename>composer.lock</filename>
        <filename>MD5SUMS</filename>
        <folder>classes</folder>
        <folder>vendor</folder>
