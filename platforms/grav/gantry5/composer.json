{
    "name": "rockettheme/gantry5",
    "type": "grav-plugin",
    "description": "Gantry Framework",
    "keywords": ["gantry"],
    "license": "MIT",

    "require": {
        "php": ">=7.3.6 <8.4.0",
        "ext-mbstring": "*",
        "ext-json": "*",
        "ext-dom": "*",
        "ext-xml": "*",
        "ext-simplexml": "*",
        "scssphp/scssphp": "^1.11.0"
    },
    "autoload": {
        "psr-4": {
            "Gantry\\": "src/classes/Gantry"
        },
        "classmap":  [
            "gantry5.php",
            "Debugger.php",
            "src/Loader.php"
        ]
    },
    "repositories": [
    ],
    "config": {
        "platform": {
            "php": "8.1"
        }
    }
}
